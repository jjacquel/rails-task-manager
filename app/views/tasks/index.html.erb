<h1>My Tasks</h1>

<% @tasks.each do |task| %>

  <p> <%= task.completed %> </p>
  <%= link_to "#{task.title}", task_path(task) %>
  <%= link_to "Delete", task_path(task), method: :delete, data: {confirm: "delete #{task.title}?"} %>
  <hr>
<% end %>

<br>

<%= link_to "Add a new task", new_task_path %>



